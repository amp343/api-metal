{
    "name": "amp343/api-metal",
    "license": "proprietary",
    "require": {
        "php": ">=7.0.4",
        "medio/inflector": "1.0.0",
        "nikic/fast-route": "0.8.0",
        "respect/validation": "1.0.5"
    },
    "require-dev": {
        "phpunit/phpunit": "4.5.*",
        "friendsofphp/php-cs-fixer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "ApiMetal\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiMetal\\Tests\\": "tests"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
